//4-1.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int num;
    scanf("%d", &num);

    for (int i = 1; i < 10; i++)
    {
        printf("%d * %d = %d\n", num, i, num * i);
    }
    return 0;
}


//4-2.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, a, b;
    char A[100], B[100];

    scanf("%d", &n);

    for (int i = 0; i < n; i++)
    {
        scanf("%d %d", &a,&b);
        A[i] = a;
        B[i] = b;
    }

    for (int i = 0; i < n; i++)
        printf("%d\n", A[i] + B[i]);
}


//4-3.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n;
    int j = 0;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++)
    {
        j = j + i;
    }
    printf("%d", j);
}


//4-4.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, i, a, b;

    scanf("%d", &n);

    for (i = 0; i < n; i++)
    {
        scanf("%d %d", &a, &b);
        printf("%d\n", a + b);
    }
}
