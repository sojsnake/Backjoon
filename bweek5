//3-5.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, i;
    scanf("%d", &n);
    for (i = 1; i <= n; i++)
        printf("%d\n", i);
}


//3-6.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, i;
    scanf("%d", &n);
    for (i = n; i > 0; i--)
        printf("%d\n", i);
}


//3-7.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, a, b;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d %d", &a, &b);
        printf("Case #%d: %d\n", i + 1, a + b);
    }
}


//3-8.c
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int n, a, b;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d %d", &a, &b);
        printf("Case #%d: %d + %d = %d\n", i + 1, a, b, a+b);
    }
}
